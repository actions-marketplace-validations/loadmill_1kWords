{
  "name": "github-action-template",
  "version": "1.0.5",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notiz-dev/github-action-template"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "typescript",
    "GitHub"
  ],
  "author": "notiz.dev",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^5.0.0",
    "@octokit/action": "^3.18.0",
    "vm2": "^3.9.7"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vercel/ncc": "^0.33.3",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  }
}
